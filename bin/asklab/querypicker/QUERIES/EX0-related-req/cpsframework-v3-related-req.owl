<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.asklab.tk/ontologies/CPS-Framework#"
     xml:base="http://www.asklab.tk/ontologies/CPS-Framework"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:CPS-Framework="http://www.asklab.tk/ontologies/CPS-Framework#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.asklab.tk/ontologies/CPS-Framework"/>

    <!-- http://www.asklab.tk/ontologies/CPS-Framework#ir_rel_0001 -->

    <owl:NamedIndividual rdf:about="http://www.asklab.tk/ontologies/CPS-Framework#ir_rel_0001">
        <rdf:type rdf:resource="http://www.asklab.tk/ontologies/CPS-Framework#ImpactRule"/>
        <addressesAtFunc rdf:resource="http://www.asklab.tk/ontologies/CPS-Framework#uc1"/>
        <addressesConcern rdf:resource="http://www.asklab.tk/ontologies/CPS-Framework#Confidentiality"/>
        <addressesPolarity rdf:resource="http://www.asklab.tk/ontologies/CPS-Framework#positive"/>
        <hasCondition rdf:resource="http://www.asklab.tk/ontologies/CPS-Framework#c_rel_0001_01"/>
        <rdfs:comment>cpsf:Integrity is addressed by property cpsf:SAMEncryptedMemAES256</rdfs:comment>
    </owl:NamedIndividual>


    <!-- http://www.asklab.tk/ontologies/CPS-Framework#c_rel_0001_01 -->

    <owl:NamedIndividual rdf:about="http://www.asklab.tk/ontologies/CPS-Framework#c_rel_0001_01">
        <rdf:type rdf:resource="http://www.asklab.tk/ontologies/CPS-Framework#Condition"/>
        <conditionPolarity rdf:resource="http://www.asklab.tk/ontologies/CPS-Framework#positive"/>
        <conditionProperty rdf:resource="http://www.asklab.tk/ontologies/CPS-Framework#SAMEncryptedMemAES256"/>
    </owl:NamedIndividual>


    <!-- http://www.asklab.tk/ontologies/CPS-Framework#SAMEncryptedMemAES256 -->

    <owl:NamedIndividual rdf:about="http://www.asklab.tk/ontologies/CPS-Framework#SAMEncryptedMemAES256">
        <rdf:type rdf:resource="http://www.asklab.tk/ontologies/CPS-Framework#Property"/>
        <hasType rdf:resource="http://www.asklab.tk/ontologies/CPS-Framework#PropertyType_Assertion"/>
        <atomicStatement>encrypted_mem_sam_aes256</atomicStatement>
        <rdfs:comment>SAM uses encrypted memory with AES-256 encryption</rdfs:comment>
    </owl:NamedIndividual>


    <!-- http://www.asklab.tk/ontologies/CPS-Framework#Confidentiality -->

    <owl:NamedIndividual rdf:about="http://www.asklab.tk/ontologies/CPS-Framework#Confidentiality">
        <rdf:type rdf:resource="http://www.asklab.tk/ontologies/CPS-Framework#Concern"/>
        <relevantToFunc rdf:resource="http://www.asklab.tk/ontologies/CPS-Framework#uc1"/>
    </owl:NamedIndividual>

</rdf:RDF>
